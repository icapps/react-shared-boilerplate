{
  "name": "react-shared-boilerplate",
  "version": "1.0.0",
  "private": true,
  "devDependencies": {
    "@babel/core": "~7.2.2",
    "@babel/plugin-proposal-nullish-coalescing-operator": "~7.2.0",
    "@babel/runtime": "~7.2.0",
    "@storybook/addon-actions": "~4.1.6",
    "@storybook/addon-links": "~4.1.6",
    "@storybook/addons": "~4.1.6",
    "@storybook/react-native": "~4.1.6",
    "babel-eslint": "10.0.1",
    "enzyme": "~3.8.0",
    "enzyme-adapter-react-16": "~1.7.1",
    "eslint": "~5.12.0",
    "eslint-config-airbnb-base": "~13.1.0",
    "eslint-plugin-import": "~2.14.0",
    "eslint-plugin-react": "~7.12.3",
    "eslint-plugin-react-native": "~3.6.0",
    "jest": "~23.6.0",
    "jest-react-native": "~18.0.0",
    "metro-react-native-babel-preset": "~0.51.1",
    "path": "^0.12.7",
    "react-dom": "~16.7.0",
    "react-native-storybook-loader": "~1.8.0",
    "react-test-renderer": "~16.7.0",
    "storybook-addon-jsx": "~6.0.0"
  },
  "scripts": {
    "start": "react-native start",
    "android": "react-native run-android --variant=debug --appIdSuffix=debug",
    "ios": "react-native run-ios",
    "lint": "eslint -c .eslintrc --ext .js src",
    "lint:fix": "eslint -c .eslintrc --ext .js src --fix",
    "prestorybook": "rnstl",
    "storybook": "storybook start -p 7007",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "validate": "yarn lint && yarn test",
    "install-utils": "cd ../shared/utils && rm -rf node_modules && rm -rf lib && yarn && yarn run build && cd ../../app",
    "install-services": "cd ../shared/services && rm -rf node_modules && rm -rf lib && yarn && yarn run build && cd ../../app",
    "install-redux": "cd ../shared/redux && rm -rf node_modules && rm -rf lib && yarn && yarn run build && cd ../../app",
    "preinstall": "yarn run install-utils && yarn run install-services && yarn run install-redux"
  },
  "jest": {
    "automock": false,
    "preset": "react-native",
    "setupFiles": [
      "./setupJest.js"
    ],
    "transform": {
      "^.+\\.js$": "./node_modules/react-native/jest/preprocessor.js"
    }
  },
  "dependencies": {
    "prop-types": "~15.6.2",
    "react": "16.7.0",
    "react-native": "~0.57.8",
    "react-native-config": "~0.11.7",
    "react-native-device-info": "~0.24.3",
    "react-native-i18n": "~2.0.15",
    "react-redux": "~6.0.0",
    "react-shared-redux": "file:../shared/redux",
    "react-shared-utils": "file:../shared/utils"
  },
  "config": {
    "react-native-storybook-loader": {
      "searchDir": [
        "./src"
      ],
      "pattern": "**/*.story.js",
      "outputFile": "./storybook/storyLoader.js"
    }
  }
}